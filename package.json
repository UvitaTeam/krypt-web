{
  "name": "@vivy/krypt-web",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/UvitaTeam/krypt-web"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist",
    "index.js",
    "README.md"
  ],
  "scripts": {
    "build:package": "babel src/lib --presets @babel/env --copy-files --out-dir dist --ignore tests",
    "prebuild": "rimraf dist",
    "build:documentation": "jsdoc src/lib && git-directory-deploy --directory out/",
    "linter:check": "eslint src/**/*",
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:watch": "karma start --auto-watch --browsers ChromeHeadless karma.conf.js"
  },
  "dependencies": {
    "scrypt-async": "2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "git-directory-deploy": "1.5.1",
    "husky": "1.3.1",
    "jsdoc": "3.5.5",
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "raw-loader": "1.0.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && pretty-quick"
    }
  }
}
